<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ASP.NET Core Doctor Appointment Scheduling Tutorial</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Project.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="header">
        <h1><a href='https://code.daypilot.org/73888/asp-net-core-doctor-appointment-scheduling-tutorial'>ASP.NET Core Doctor Appointment Scheduling Tutorial (.NET 6)</a></h1>
        <div><a href="https://javascript.daypilot.org/">DayPilot for JavaScript</a> - HTML5 Calendar/Scheduling Components for JavaScript/Angular/React/Vue</div>
    </div>

    <nav>
        <a href="./">Patient</a>
        |
        <a href="./Doctor">Doctor</a>
        |
        <a href="./Manager">Manager</a>
    </nav>

    <div class="main">
        @RenderBody()
    </div>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>